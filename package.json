{
  "name": "girder.io",
  "version": "1.0.0",
  "description": "web app",
  "main": "index.js",
  "git repository": "git@github.com:Samuyi/Girder.io.git",
  "scripts": {
    "start": "nodemon -e js,jsx --ignore lib --ignore dist --exec babel-node src/server",
    "test": "eslint src && flow && jest --coverage",
    "dev:wds": "webpack-dev-server --progress",
    "lint": "eslint src webpack.config.babel.js --ext .js, .jsx",
    "precommit": "npm test",
    "prepush": "npm test && npm run prod:start",
    "prod:build": "rimraf lib && babel src -d lib --ignore .test.js && cross-env NODE_ENV=production webpack -p --progress",
    "prod:start": "cross-env NODE_ENV=production pm2 start lib/server && pm2 logs",
    "prod:stop": "pm2 delete server"
  },
  "browserlist": [
    "> 1%"
  ],
  "author": "Samuyi",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.2.1",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-flow-react-proptypes": "^0.21.0",
    "babel-preset-env": "^1.3.2",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.23.0",
    "cross-env": "^4.0.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-compat": "^1.0.2",
    "eslint-plugin-flowtype": "^2.30.4",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "fetch-mock": "^5.9.4",
    "flow-bin": "^0.42.0",
    "husky": "^0.13.3",
    "jest": "^19.0.2",
    "nodemon": "^1.11.0",
    "pm2": "^2.4.3",
    "redux-mock-store": "^1.2.2",
    "rimraf": "^2.6.1",
    "webpack": "^2.3.3",
    "webpack-dev-server": "^2.4.2"
  },
  "dependencies": {
    "bootstrap": "^4.0.0-alpha.6",
    "compression": "^1.6.2",
    "express": "^4.15.2",
    "immutable": "^3.8.1",
    "isomorphic-fetch": "^2.2.1",
    "jquery": "^3.2.1",
    "react": "^15.5.3",
    "react-dom": "^15.5.3",
    "react-helmet": "^5.0.2",
    "react-hot-loader": "^1.3.1",
    "react-jss": "^6.0.0",
    "react-redux": "^5.0.3",
    "react-router": "^4.0.0-beta.8",
    "react-router-dom": "^4.0.0-beta.8",
    "redux": "^3.6.0",
    "redux-actions": "^2.0.1",
    "redux-thunk": "^2.2.0",
    "socket.io": "^1.7.3",
    "socket.io-client": "^1.7.3",
    "tether": "^1.4.0"
  }
}
